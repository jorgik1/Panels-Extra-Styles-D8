<?php

/**
 * @file
 * Contains panels_extra_styles.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function panels_extra_styles_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the panels_extra_styles module.
    case 'help.page.panels_extra_styles':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Additional styles for the Panels module.') . '</p>';
      return $output;

    default:
  }
}
